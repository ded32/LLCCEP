class Point {
	public Real x, y
}

class Window <- window {
	public void Create(int w, int h, String title)
	public void Show()
	public void Hide()

	public void setPixel(Point p)
	public COLOR getPixel(Point p)
}

function main(Real argn, String argv[]) {
	for Real i = 1; i < argn; i++ {
		print('Argument #{i}: #{argv[i]}')
	}

	return argn >= 1
}
